<% case @navbar %>
<% when "garden" %>
  <%= render "shared/top_navbar", mission_name: "UU Gardening Team", mission_path: gardens_path do %>
    <li><%= link_to "Home", gardens_path %></li>
    <li><%= link_to "New Report", new_garden_report_path %></li>
    <li><%= link_to "New Garden", new_garden_path %></li>
    <li><%= link_to "Invite Gardener", new_user_invitation_path %></li>
  <% end %>
<% else %>
  <%= render "shared/top_navbar", mission_name: "Project Jorge", mission_path: root_path do %>
  <% end %>
<% end %>
